{
  "name": "@essif-lab/trrt",
  "version": "0.0.2",
  "description": "Term Reference Resolution Tool (TRRT)",
  "main": "lib/Run.js",
  "types": "lib/Run.d.ts",
  "bin": {
    "trrt": "./lib/Run.js"
  },
  "type": "module",
  "scripts": {
    "test": "jest --config jest.config.json",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' Run.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" Run.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && trrt",
    "rebuild": "npm uninstall trrt -g && rm lib -rf && npm run build && npm i -g",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "package": "ncc build --source-map --license licenses.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/essif-lab/trrt"
  },
  "keywords": [],
  "author": "eSSIF-Lab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/essif-lab/trrt/issues"
  },
  "homepage": "https://github.com/essif-lab/trrt#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "0.1.0",
    "commander": "10.0.0",
    "download": "^8.0.0",
    "figlet": "^1.6.0",
    "js-yaml": "^4.1.0",
    "tslog": "^4.4.4"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/download": "^8.0.1",
    "@types/figlet": "^1.5.5",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.18",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib/**/*"
  ]
}
